{
  "name": "ai-agent-platform",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run backend:start\" \"vite\"",
    "frontend:dev": "vite",
    "backend:dev": "cd backend && npm run dev",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "docker:build": "docker-compose build",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "docker:shell": "docker exec -it ai-agent-platform bash",
    "backend:install": "cd backend && npm install",
    "setup": "npm install && npm run backend:install",
    "deploy:render": "node deployment/deploy-to-render.js",
    "deploy:instructions": "node deployment/deploy-to-render.js --instructions",
    "backend:start": "cd backend && npm run dev",
    "backend:cloud": "cd backend && npm run start"
  },
  "dependencies": {
    "@huggingface/inference": "^4.3.2",
    "@monaco-editor/react": "^4.6.0",
    "axios": "^1.10.0",
    "lucide-react": "^0.263.1",
    "node-fetch": "^3.3.2",
    "prismjs": "^1.29.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-markdown": "^8.0.7",
    "remark-gfm": "^3.0.1",
    "zustand": "^4.4.1"
  },
  "devDependencies": {
    "@gradio/client": "^1.15.4",
    "@types/node": "^20.0.0",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react": "^4.0.3",
    "autoprefixer": "^10.4.14",
    "concurrently": "^8.2.0",
    "eslint": "^8.45.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "postcss": "^8.4.27",
    "tailwindcss": "^3.3.3",
    "typescript": "^5.0.2",
    "vite": "^4.4.5"
  }
}
